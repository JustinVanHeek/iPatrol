<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dev.ipatrol">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NewPatrol" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".activityreports.GraffitiReportActivity"></activity>
        <activity android:name=".activityreports.ParkingMeterReportActivity"></activity>
        <activity android:name=".activityreports.PoliceCallReportActivity"></activity>
        <activity android:name=".activityreports.RecklessDrivingReportActivity"></activity>
        <activity android:name=".activityreports.SpeedingReportActivity"></activity>
        <activity android:name=".activityreports.StreetPerformingReportActivity"></activity>
        <activity android:name=".activityreports.TFAGlassReportActivity"></activity>
        <activity android:name=".activityreports.AbandonedCarReportActivity"></activity>
        <activity android:name=".activityreports.AlcoholReportActivity"></activity>
        <activity android:name=".activityreports.CrimePreventionReportActivity"></activity>
        <activity android:name=".activityreports.DrugUseReportActivity"></activity>
        <activity android:name=".activityreports.IllegalDumpingReportActivity"></activity>
        <activity android:name=".activityreports.MischiefReportActivity"></activity>
        <activity android:name=".activityreports.OffLeashReportActivity"></activity>
        <activity android:name=".activityreports.OtherReportActivity"></activity>


    </application>

</manifest>